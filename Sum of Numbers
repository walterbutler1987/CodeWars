  public class Sum  {
     public int GetSum(int a, int b)     {
       int sum = 0;
       if(a < b){
       for(int i = a; i <= b; i++){
         sum += i;
         }
       } else if( b < a){
         for(int i = b; i <= a; i++){
          sum += i;
         }
       } else {
         return a;
       }
      
      return sum;
       
     }
  }
